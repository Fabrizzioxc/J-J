import PageHero from '@components/ui/PageHero.astro';
import MaintenanceComingSoon from '@components/ui/MaintenanceComingSoon.astro';
---

<Layout
    title="Nuestros Proyectos | Próximamente - J&J Servicios Generales"
    description="Descubre los proyectos realizados por J&J Servicios Generales en Drywall, Pintura y Electricidad en Lima. ¡Próximamente estaremos compartiendo nuestras obras más destacadas!"
>
    <PageHero
        title="Proyectos"
        breadcrumbs={[
            { label: 'Inicio' },
            { label: 'Proyectos' },
        ]}
    />
  <ServicesBanner/>
  <MaintenanceComingSoon />
  <ServicesBanner/>
