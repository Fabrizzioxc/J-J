---
import Layout from '@layouts/Layout.astro';
import Header from '@components/Header.astro';
import ServicesBanner from '@components/ui/ServicesBanner.astro'; // Ajusta la ruta si es necesario
import PageHero from '@components/ui/PageHero.astro';
import Footer from '@components/Footer.astro';
import ContactSection from '@components/ContactSection.astro';
import MapEmbed from '@components/ui/MapEmbed.astro';

---

<Layout title="Servicios - J&J Servicios Generales">
  <Header />
  <PageHero
        title="Contacto"
        breadcrumbs={[
            { label: 'Inicio' },
            { label: 'Contacto' },
        ]}
    />
  <ServicesBanner/>
  <ContactSection />
  <MapEmbed />
  <ServicesBanner/>
  <Footer />
</Layout>